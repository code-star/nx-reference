{
    "version": 1,
    "projects": {
        "balance": {
            "root": "apps/balance",
            "sourceRoot": "apps/balance/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": ["{options.outputPath}"],
                    "defaultConfiguration": "production",
                    "options": {
                        "outputPath": "dist/apps/balance",
                        "index": "apps/balance/src/index.html",
                        "baseHref": "/",
                        "main": "apps/balance/src/main.tsx",
                        "polyfills": "apps/balance/src/polyfills.ts",
                        "tsConfig": "apps/balance/tsconfig.app.json",
                        "assets": [
                            "apps/balance/src/favicon.ico",
                            "apps/balance/src/assets"
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "apps/balance/webpack.config.js"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/balance/src/environments/environment.ts",
                                    "with": "apps/balance/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "balance:build",
                        "hmr": true,
                        "port": 4202
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "balance:build:production",
                            "hmr": false
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": [
                            "apps/balance/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/balance"],
                    "options": {
                        "jestConfig": "apps/balance/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "balance-e2e": {
            "root": "apps/balance-e2e",
            "sourceRoot": "apps/balance-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/balance-e2e/cypress.json",
                        "devServerTarget": "balance:serve",
                        "tsConfig": "apps/balance-e2e/tsconfig.json"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "balance:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["apps/balance-e2e/**/*.{js,ts}"]
                    }
                }
            },
            "tags": [],
            "implicitDependencies": ["balance"]
        },
