{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "master" },
  "npmScope": "star",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "btc": { "tags": [] },
    "demo": { "tags": [] },
    "demo-e2e": { "tags": [], "implicitDependencies": ["demo"] },
    "server": { "tags": [] },
    "ui": { "tags": [] },
    "ui-e2e": { "tags": [], "implicitDependencies": ["ui"] },
    "shared-types": { "tags": [] },
    "shared-services": { "tags": [] },
    "shared-data-access": { "tags": [] }
  }
}
